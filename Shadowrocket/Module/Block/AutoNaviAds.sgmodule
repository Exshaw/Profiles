#!name=AutoNavi Advertising
#!desc=Block AutoNavi Advertising (Oct 08 2025)
#!url=https://raw.githubusercontent.com/Exshaw/Profiles/master/Shadowrocket/Module/Block/AutoNaviAds.sgmodule
# @ddgksf2013: https://github.com/ddgksf2013/Rewrite/blob/master/AdBlock/AmapAds.conf

[Rule]
DOMAIN-SUFFIX,optimus-ads.amap.com,DIRECT

[URL Rewrite]
^https?:\/\/.*\.amap\.com\/ws\/aos\/alimama\/splash_screen - reject-200
^https?:\/\/.*\.amap\.com\/ws\/faas\/amap-navigation\/card-service-route-plan\? - reject-dict
^https?:\/\/.*\.amap\.com\/ws\/boss\/order_web\/\w{8}_information - reject-200
^https?:\/\/.*\.amap\.com\/ws\/asa\/ads_attribution - reject
^https?:\/\/.*\.amap\.com\/ws\/shield\/scene\/recommend - reject-dict
^https?:\/\/.*\.amap\.com\/uploadimg\/\w+\.gif - reject-img
^https?:\/\/.*\.amap\.com\/ws\/valueadded\/weather - reject-dict

[Script]
AMDC = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^http:\/\/amdc\.m\.taobao\.com, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amdc.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/message\/notice\/list, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/shield\/frogserver\/aocs\/updatable, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/shield\/search\/nearbyrec_smart, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/promotion-web\/resource, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/faas\/amap-navigation\/main-page, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/valueadded\/alimama\/splash_screen, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/msgbox\/pull, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/shield\/dsp\/profile\/index\/nodefaas, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js
AutoNavi = type=http-response, requires-body=true, max-size=-1, timeout=60, pattern=^https?:\/\/.*\.amap\.com\/ws\/shield\/search\/new_hotword, script-path=https://raw.githubusercontent.com/ddgksf2013/Scripts/refs/heads/master/amap.js

